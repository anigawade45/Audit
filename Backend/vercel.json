{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run build", 
  "installCommand": "npm install",
  "functions": {
    "server.js": {
      "includeFiles": ["dist/**"]
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/server.js"
    }
  ]
}
